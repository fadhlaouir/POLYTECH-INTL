      "collection": "user",
      "via": "courses",
      "dominant": true
    },
    "rooms": {
      "collection": "room",
      "via": "courses",
      "dominant": true
    },
    "departments": {
      "collection": "department",
      "via": "courses",
      "dominant": true
    }
  }
}
