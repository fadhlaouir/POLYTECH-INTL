{
  "kind": "collectionType",
  "collectionName": "levels",
  "info": {
    "name": "Level"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "departments": {
      "collection": "department",
      "via": "levels",
      "dominant": true
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "level"
    },
    "groups": {
      "via": "level",
      "collection": "group"
    }
  }
}
